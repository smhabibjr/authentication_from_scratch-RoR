<div class="container">
  <h1>Rails application</h1>

  <%= form_with model: @members, url: sign_up_path do |f| %>
  <p>
    <%= f.label 'email' %> <br>
    <%= f.text_field :email %>
  </p>

    <p>
      <%= f.label 'Password' %> <br>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.label 'Password Confirmation' %> <br>
      <%= f.password_field :password_confirmation %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>

</div>