<div class="container">
  <h1>Sign up</h1>
  <%= form_with model: @students, url: sign_up_path do |f| %>
  <p>
    <%= f.label "email" %> <br>
    <%= f.text_field :email %>
  </p>

    <p>
      <%= f.label "Password" %> <br>
      <%= f.password_field :password %>
    </p>

    <p>
      <%= f.label "Password confirmation" %> <br>
      <%= f.password_field :password_confirmation %>
    </p>

    <p>
      <%= f.submit "Sign up" %>
    </p>

  <% end %>
</div>
